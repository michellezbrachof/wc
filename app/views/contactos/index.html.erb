<div class="container-fluid text-center">
  <p id="notice"><%= notice %></p>

  <h1>Contactos</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Service type</th>
        <th>Stage</th>
        <th>Budget</th>
        <th>Timeframe</th>
        <th>Start</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @contactos.each do |contacto| %>
        <tr>
          <td><%= contacto.name %></td>
          <td><%= contacto.email %></td>
          <td><%= contacto.phone %></td>
          <td><%= contacto.service_type %></td>
          <td><%= contacto.stage %></td>
          <td><%= contacto.budget %></td>
          <td><%= contacto.timeframe %></td>
          <td><%= contacto.start %></td>
          <td><%= link_to 'Show', contacto %></td>
          <td><%= link_to 'Edit', edit_contacto_path(contacto) %></td>
          <td><%= link_to 'Destroy', contacto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Contacto', new_contacto_path %>
</div>