<p id="notice"><%= notice %></p>

<h1>On Touch Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Prefix</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @on_touch_users.each do |on_touch_user| %>
      <tr>
        <td><%= on_touch_user.name %></td>
        <td><%= on_touch_user.prefix %></td>
        <td><%= on_touch_user.email %></td>
        <td><%= link_to 'Show', on_touch_user %></td>
        <td><%= link_to 'Edit', edit_on_touch_user_path(on_touch_user) %></td>
        <td><%= link_to 'Destroy', on_touch_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New On Touch User', new_on_touch_user_path %>
